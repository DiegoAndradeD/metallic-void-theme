	"tokenColors": [
		{
			"scope": "comment",
			"settings": {
				"foreground": "#404040",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["keyword", "storage", "keyword.operator", "keyword.control"],
			"settings": {
				"foreground": "#a64952",
				"fontStyle": ""
			}
		},
		{
			"scope": ["variable", "variable.parameter", "variable.other.object", "variable.language"],
			"settings": {
				"foreground": "#889fa1"
			}
		},
		{
			"scope": ["string", "constant.numeric"],
			"settings": {
				"foreground": "#b07b68"
			}
		},
		{
			"scope": ["entity.name.function", "support.function", "meta.function-call"],
			"settings": {
				"foreground": "#d8dee9"
			}
		},
		{
			"scope": ["entity.name.type", "entity.name.class", "support.class"],
			"settings": {
				"foreground": "#c9b385"
			}
		},
		{
			"scope": ["markup.heading", "entity.name.tag"],
			"settings": {
				"foreground": "#a64952"
			}
		},
		{
			"scope": ["punctuation", "meta.brace"],
			"settings": {
				"foreground": "#4c566a"
			}
		},
		{
			"scope": ["entity.other.attribute-name", "meta.tag.attributes"],
			"settings": {
				"foreground": "#b48ead",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["support.class.component", "entity.name.tag.custom"],
			"settings": {
				"foreground": "#ebcb8b"
			}
		},
		{
			"scope": ["meta.jsx.children", "text.html.basic"],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"scope": ["variable.other.property", "support.variable.property", "meta.object-literal.key"],
			"settings": {
				"foreground": "#b48ead"
			}
		},
		{
			"scope": ["meta.member.access.typescript"],
			"settings": {
				"foreground": "#b48ead"
			}
		}
	],
	"semanticHighlighting": true,
	"semanticTokenColors": {
		"parameter": {
			"foreground": "#d08770",
			"fontStyle": "italic"
		},
		"variable.readonly": {
			"foreground": "#b48ead"
		},
		"variable.constant": {
			"foreground": "#b48ead"
		},
		"interface": {
			"foreground": "#a3be8c",
			"fontStyle": "italic"
		},
		"typeParameter": {
			"foreground": "#bf616a",
			"fontStyle": "bold"
		},
		"function": "#d8dee9",
		"method": "#d8dee9",
		"property": "#b48ead",
		"property.readonly": "#b48ead"
	}
}
